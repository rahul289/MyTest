/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */

class Codechef 
{
    public class BT{
        int data;
        BT left,right;
        public BT(int data)
        {
            this.data=data;
            this.left=null;
            this.right=null;
        }
        public void setLeft(BT left)
        {
            this.left=left;
        }
        public BT getLeft()
        {
            return left;
        }
        public void setRight(BT right)
        {
            this.right=right;
        }
        public BT getRight()
        {
            return right ;
        }
    }

	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		binaryTree();
	
		
	}
	public void binaryTree()
	{
	    	BT tree=new BT(5);
		BT ltree=new BT(4); 
		BT rtree=new BT(6);
		tree.setLeft(ltree);
		tree.setRight(rtree);
		System.out.println("Tree data is "+tree.getLeft().data);
	}
}
